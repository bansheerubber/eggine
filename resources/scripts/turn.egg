class EndTurnElement inherits HTMLElement {
	function onClick(%this) {
		endTurn();
	}
};

function endTurn() {
	%team = getChunkContainer().getPlayerTeam();
	for(%i = 0; %i < %team.size(); %i++) {
		%team.get(%i).setAP(2);
	}

	%team = getChunkContainer().getEnemyTeam();
	for(%i = 0; %i < %team.size(); %i++) {
		%team.get(%i).setAP(2);
	}
}
